   ],
   "source": [
    "number = int(input('Enter the number: '))\n",
    "result = 1\n",
    "\n",
    "if number<0:\n",
    "    print('There is no factorial of a number')\n",
    "else:\n",
    "    while number>=1:\n",
    "        result = result*number\n",
    "        number=number-1\n",
    "    print(result)  "
   ]
