   "source": [
    "# Sum of Digit of a Number Without Recursion in Python"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "87179daa",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a number: 34\n",
      "Sum is:\n",
      "7\n"
     ]
    }
   ],
   "source": [
    "l=[]\n",
    "b=int(input(\"Enter a number: \"))\n",
    "while(b>0):\n",
    "    dig=b%10\n",
    "    l.append(dig)\n",
    "    b=b//10\n",
    "print(\"Sum is:\")\n",
    "print(sum(l))"
   ]
  }
